{
  "name": "otus_react_project",
  "version": "1.0.0",
  "description": "Project from Otus courses",
  "main": "index.js",
  "scripts": {
    "test": "npx jest",
    "test:watch": "npx jest --watch",
    "build": "npx webpack --mode production",
    "start": "webpack-dev-server --config ./webpack.config.js --mode development --open --hot",
    "lint": "npx eslint --ext .js,.jsx,.ts,.tsx --fix src/",
    "storybook": "start-storybook -p 6008",
    "build-storybook": "build-storybook",
    "loki:update": "npm run build-storybook && npx loki update --requireReference --reactUri file:./storybook-static",
    "loki:approve": "npx loki approve",
    "test:loki": "npm run build-storybook && npx loki --requireReference --reactUri file:./storybook-static"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alietta/otus_react_project.git"
  },
  "author": "Tatiana Kudashova",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/alietta/otus_react_project/issues"
  },
  "homepage": "https://github.com/alietta/otus_react_project#readme",
  "husky": {
    "hooks": {
      "pre-commit": "npm test && npm run lint",
      "pre-push": "npm test && npm run lint"
    }
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.0",
    "@babel/preset-react": "^7.9.4",
    "@babel/preset-typescript": "^7.9.0",
    "@emotion/babel-preset-css-prop": "^10.0.27",
    "@storybook/addon-actions": "^5.3.18",
    "@storybook/addon-knobs": "^5.3.18",
    "@storybook/addon-links": "^5.3.18",
    "@storybook/addon-storysource": "^5.3.18",
    "@storybook/addons": "^5.3.18",
    "@storybook/react": "^5.3.18",
    "@types/enzyme": "^3.10.5",
    "@types/react-test-renderer": "^16.9.2",
    "@typescript-eslint/eslint-plugin": "^2.26.0",
    "@typescript-eslint/parser": "^2.26.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-emotion": "^10.0.33",
    "case-sensitive-paths-webpack-plugin": "^2.3.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "enzyme-to-json": "^3.4.4",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.19.0",
    "html-webpack-plugin": "^4.0.4",
    "husky": "^4.2.3",
    "jest": "^25.2.6",
    "jest-canvas-mock": "^2.2.0",
    "jest-emotion": "^10.0.32",
    "jest-localstorage-mock": "^2.4.2",
    "jest-serializer-enzyme": "^1.0.0",
    "loki": "^0.20.3",
    "prettier": "^2.0.2",
    "react-test-renderer": "^16.13.1",
    "redux-mock-store": "^1.5.4",
    "redux-saga-test-plan": "^4.0.0-rc.3",
    "storybook": "^5.3.18",
    "typescript": "^3.8.3",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3"
  },
  "dependencies": {
    "@babel/polyfill": "^7.8.7",
    "@emotion/cache": "^10.0.29",
    "@emotion/core": "^10.0.28",
    "@emotion/css": "^10.0.27",
    "@emotion/styled": "^10.0.27",
    "@reduxjs/toolkit": "^1.3.6",
    "apollo-boost": "^0.4.9",
    "axios": "^0.19.2",
    "axios-jsonp": "^1.0.4",
    "axios-jsonp-pro": "^1.1.8",
    "cross-fetch": "^3.0.5",
    "graphql": "^15.3.0",
    "query-string": "^6.13.1",
    "react": "^16.13.1",
    "react-apollo": "^3.1.5",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.0",
    "react-router-dom": "^5.2.0",
    "redux-devtools-extension": "^2.13.8",
    "redux-saga": "^1.1.3",
    "sancho": "^3.5.6"
  },
  "loki": {
    "configurations": {
      "chrome.laptop": {
        "target": "chrome.docker",
        "width": 1366,
        "height": 768,
        "deviceScaleFactor": 1,
        "mobile": false
      },
      "chrome.iphone7": {
        "target": "chrome.docker",
        "preset": "iPhone 7"
      }
    }
  }
}
